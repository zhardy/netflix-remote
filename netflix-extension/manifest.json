{
  "manifest_version": 2,

  "name": "Netflix Remote",
  "description": "Allows you to navigate throughout a specific channel",
  "version": "1.0",
  "permissions": [
    "tabs" ,
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["jquery-2.1.4.min.js","ajax.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }

}