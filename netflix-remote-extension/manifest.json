{
  "manifest_version": 2,

  "name": "Netflix Remote",
  "description": "Allows you to navigate throughout a specific channel",
  "version": "1.0",
  "permissions": [
    "tabs" ,
    "http://localhost:3000/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }

}